                    </div><!-- /.col-lg-6 -->
                  </div><!-- /.row -->
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button id="btn_DropdownSave" type="button" class="btn btn-primary">Agregar</button>
              </div>
            </div>
          </div>
