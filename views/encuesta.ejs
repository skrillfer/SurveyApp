      <!-- Modal -->
      <div class="modal" id="modalGrafica" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Selecciona la Columna</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
